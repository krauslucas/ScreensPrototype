<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" fx:id="background" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Standard.ReportNoteController">
   <children>
      <Label fx:id="tab" layoutX="22.0" layoutY="15.0" scaleX="1.25" scaleY="1.25" text="Informar Nota" />
      <Label fx:id="name" layoutX="94.0" layoutY="100.0" scaleX="1.5" scaleY="1.5" text="Nome: Prova Java" />
      <Label fx:id="discipline" layoutX="130.0" layoutY="140.0" scaleX="1.5" scaleY="1.5" text="Disciplina: Programação Orientada a Objetos" />
      <Label fx:id="average" layoutX="85.0" layoutY="180.0" scaleX="1.5" scaleY="1.5" text="Média: M1" />
      <Label fx:id="myNote" layoutX="87.0" layoutY="260.0" scaleX="1.5" scaleY="1.5" text="Minha nota:" />
      <TextField fx:id="note" layoutX="70.0" layoutY="290.0" />
      <Button fx:id="back" layoutX="40.0" layoutY="540.0" mnemonicParsing="false" onAction="#backScreenMyTests" prefWidth="60.0" scaleX="1.25" scaleY="1.25" text="Voltar" />
      <Button fx:id="save" layoutX="690.0" layoutY="540.0" mnemonicParsing="false" onAction="#saveNote" prefWidth="60.0" scaleX="1.25" scaleY="1.25" text="Salvar" />
   </children>
</AnchorPane>
